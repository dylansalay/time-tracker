<td id=<%="work_order_#{tech.work_order(time)&.id}"%> data-toggle='modal' data-target=<%= "order-modal-#{tech.work_order(time)&.id}"%>><%= order_details(tech.work_order(time)) %></td>

<div id="modal-<%= tech.work_order(time)&.id %>" class="modal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5> Work Order #<%= tech.work_order(time)&.id %></h5>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
      <div class="modal-body">
        <h5><strong>Tech:</strong> <%= tech.name %></h5>
        <h5><strong>Location:</strong> <%= tech.work_order(time)&.order_location %></h5>
        <h5><strong>Duration:</strong> <%= tech.work_order(time)&.duration %> Minutes</h5>
      </div>
    </div>
  </div>
</div>
